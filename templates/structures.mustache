{{license_header}}
$version: "2"
namespace OpenSearch

structure {{common_query_structure}} {
{{#query_params}}
  @httpQuery("{{name}}")
  {{#unique_description?}}
  @documentation("{{{description}}}")
  {{/unique_description?}}
  {{#unique_default?}}
  @default({{{default}}})
  {{/unique_default?}}
  {{#required?}}
  @required
  {{/required?}}
  {{name}}: {{type}},
  {{#_blank_line}}

  {{/_blank_line}}
{{/query_params}}
}

{{#common_body_structure}}
// TODO: Fill in Body Parameters
structure {{common_body_structure}} {}
{{/common_body_structure}}

{{#input_structures}}
@input
structure {{structure_name}} with [{{common_query_structure}}{{#with_body}}, {{common_body_structure}}{{/with_body}}] {
  {{#path_params}}
  @required
  @httpLabel
  {{name}}: {{type}},
  {{#_blank_line}}

  {{/_blank_line}}
  {{/path_params}}
}

{{/input_structures}}
@output
structure {{output_structure}} {}
